<%= simple_form_for(@tagging) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tag_id %>
    <%= f.input :taggable_type %>
    <%= f.input :taggable_id %>
    <%= f.input :tagger_type %>
    <%= f.input :tagger_id %>
    <%= f.input :context %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
