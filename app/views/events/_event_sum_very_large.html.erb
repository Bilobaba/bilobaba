<a href=<%= event.url %> >
  <div class="course-block-wrapper list-view w-clearfix">
    <div class="course-image-link-block list-view w-inline-block"  style="background-image:url(<%= event.image_url %>)" >
      <div class="image-overlay-block"></div>
    </div>
    <div class="featured-label"><%= event.soon %> Coucou</div>
    <div class="course-content-block list-view"><div class="course-title-link list-view" href=<%= event.url %> ><%= event.title%></div></div>
    <div class="course-content-block _2 list-view"><img class="teacher-overlay-photo list-view-photo w-hidden-tiny" src=<%= event.organizer.avatar %>>
      <div class="teacher-overlay-title list-view w-hidden-tiny"><%= event.organizer.pseudo %></div>
      <div class="course-info-icon"></div>
      <div class="event-info-title"><%= event.show_begin_at %></div>
      <div class="course-info-icon"></div>
      <div class="event-info-title"><%= event.zip %> <%= event.city %></div>
    </div>
  </div>
</a>
