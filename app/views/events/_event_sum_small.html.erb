        <%= event.short_title %>
      </div>
    </div>
    <div class="course-content-block _2">
      <div class="course-info-icon"><i class="material-icons" style="font-size: 16px; position:relative; top:3px">place</i></div>
      <div class="course-info-title"><%= event.zip %></div>
      <div class="course-info-icon">ï€—</div>
      <div class="course-info-title"><%= event.show_begin_at_hours %></div>
    </div>
  </div>
</div>
