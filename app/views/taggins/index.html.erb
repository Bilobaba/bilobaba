<p id="notice"><%= notice %></p>

<h1>Taggins</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Taggable type</th>
      <th>Taggable</th>
      <th>Tagger type</th>
      <th>Tagger</th>
      <th>Context</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @taggins.each do |taggin| %>
      <tr>
        <td><%= taggin.tag_id %></td>
        <td><%= taggin.taggable_type %></td>
        <td><%= taggin.taggable_id %></td>
        <td><%= taggin.tagger_type %></td>
        <td><%= taggin.tagger_id %></td>
        <td><%= taggin.context %></td>
        <td><%= link_to 'Show', taggin %></td>
        <td><%= link_to 'Edit', edit_taggin_path(taggin) %></td>
        <td><%= link_to 'Destroy', taggin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Taggin', new_taggin_path %>
