<!DOCTYPE html>
<html>
  <head>

    <html data-wf-page="5ab4d2321738f455ba7f5cae" data-wf-site="5a8d8e93b7a50a0001e1b4fe">

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-112119547-1"></script>

    <!-- jquery & Calendar-->
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>


    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-112119547-1');
    </script>

    <title>Bilobaba</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= favicon_link_tag 'favicon.png' %>

    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/instantsearch.js@1/dist/instantsearch.min.css">

    <script src="https://cdn.jsdelivr.net/npm/instantsearch.js@1/dist/instantsearch.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/instantsearch.js@1/dist/instantsearch-preact.min.js"></script>

    <%= stylesheet_link_tag "jquery.calendars.picker.css" %>
    <%= javascript_include_tag "jquery.plugin.js" %>
    <%= javascript_include_tag "jquery.calendars.js" %>
    <%= javascript_include_tag "jquery.calendars.all.js" %>
    <%= javascript_include_tag "jquery.calendars.plus.js" %>
    <%= javascript_include_tag "jquery.calendars.picker.js" %>
    <%= javascript_include_tag "jquery.datepick-fr.js" %>

    <%= stylesheet_link_tag "normalize.css" %>
    <%= stylesheet_link_tag "webflow.css" %>
    <%= stylesheet_link_tag "duys-trendy-project.webflow.css" %>

    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.4.7/webfont.js" type="text/javascript"></script>
    <script type="text/javascript">WebFont.load({  google: {    families: ["Source Sans Pro:200,300,regular,italic,600,700,900"]  }});</script>
    <!-- [if lt IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.3/html5shiv.min.js" type="text/javascript"></script><![endif] -->
    <script type="text/javascript">!function(o,c){var n=c.documentElement,t=" w-mod-";n.className+=t+"js",("ontouchstart"in o||o.DocumentTouch&&c instanceof DocumentTouch)&&(n.className+=t+"touch")}(window,document);</script>
  </head>

  <% if current_page?(home_path) %>
  <body id="background_home">
    <%= render '/layouts/navbar' %>
  <% else %>
  <body>
    <div id="background_top">
      <%= render '/layouts/navbar' %>
      <h1><%= @h1_title %></h1>
    </div>
    <%= render 'shared/flashes' %>
  <% end %>

    <%= yield %>

    <% if !current_page?(home_path) %>
      <%= render '/layouts/footer' %>
    <% end %>


    <%= render "/pages/info_welcome" %>
    <!-- Popup showed if the first time -->
    <% if !cookies.permanent[VIEWED_ANY] %>
      <script>
         $('#modal_info').modal('show');
      </script>
    <% end %>

    <!-- load the google map API, only once (see data-turbolinks-eval = false) -->
    <%= javascript_include_tag 'https://maps.googleapis.com/maps/api/js?key=' + ENV['GOOGLE_API'] + '&libraries=places&callback=initAutocomplete', async: true, defer: true, 'data-turbolinks-eval': false %>
    <%= javascript_include_tag 'map.js' %>
    <%= javascript_include_tag 'webflow.js' %>
  </body>
</html>
