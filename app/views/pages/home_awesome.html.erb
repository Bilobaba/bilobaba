  <div class="section">
    <div class="container w-container">
      <div class="section-title-wrapper">
        <h2 class="section-title">Prochaines propositions</h2>
        <div class="section-title-divider"></div>
      </div>
      <div class="w-dyn-list">
        <div class="w-clearfix w-dyn-items w-row" >
          <% for i in 0..3 %>
            <% if !@list_next_events_4[0].nil? %>
              <div class="featured-course-item w-dyn-item w-col w-col-3">
                <div class="course-block-wrapper home-featured">
                  <a href=<%= @list_next_events_4[i].url %> style="background-image:url(<%= @list_next_events_4[i].image_url %>)" class="course-image-link-block home-featured w-inline-block" >
                    <div class="teacher-overlay-block w-clearfix">
<!--                       <img class="teacher-overlay-photo">
 -->                      <%= cl_image_tag @list_next_events_4[i].organizer.avatar, class: "teacher-overlay-photo" %>
                      <div class="teacher-overlay-title"><%= @list_next_events_4[i].organizer.pseudo %></div>
                    </div>
                    <div class="image-overlay-block">
                      <div class="image-overlay-block w-clearfix">
                        <div class="featured-label">Featured</div>
                      </div>
                    </div>
                  </a>
                  <div class="course-content-block">
                    <%= link_to @list_next_events_4[i].title[0..42], home_path, class: "course-title-link" %>
                  </div>
                  <div class="course-content-block _2">
                    <div class="course-info-icon"><i class="material-icons" style="font-size: 16px; position:relative; top:3px">place</i></div>
                    <div class="course-info-title"><%= @list_next_events_4[i].zip %></div>
                    <div class="course-info-icon"></div>
                    <div class="course-info-title"><%= @list_next_events_4[i].show_begin_at_hours %></div>
                  </div>
                </div>
              </div>
            <% end %>
          <% end %>
        </div>
        <div class="empty-state w-dyn-empty">
          <p>No other featured courses found.</p>
        </div>
      </div>
      <div class="bottom-info-text">Want to see the full list of our Courses? <a href="our-courses/courses-grid-view.html">View Full Courses Overview →</a></div>
    </div>
  </div>
