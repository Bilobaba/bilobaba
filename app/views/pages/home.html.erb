<div class="header-wrapper">
  <div data-collapse="none" data-animation="default" data-duration="400" class="top-navbar w-nav">
    <div class="w-container">
      <nav role="navigation" class="top-nav-menu w-nav-menu">
        <%= link_to contact_path, class: 'top-nav-link w-inline-block' do %>
          <div class="top-nav-title icon"></div>
          <div class="top-nav-title">Nous contacter</div>
        <% end %>
      </nav>
      <%= link_to 'Accédez aux témoignages →', home_path, class: 'top-bar-link w-hidden-small w-hidden-tiny' %>
      <div class="w-nav-button">
        <div class="w-icon-nav-menu"></div>
      </div>
    </div>
  </div>
  <div data-collapse="medium" data-animation="over-right" data-duration="400" data-doc-height="1" class="navbar w-nav">
    <div class="w-container">
      <%= link_to home_path, class: 'logo-block w-nav-brand' do %>
        <%= image_tag 'logo.png', class:"image-logo" %>
      <% end %>
      <nav role="navigation" class="nav-menu w-nav-menu">
        <%= link_to 'Les propositions', events_path, class: 'nav-link w-nav-link' %>
        <div data-delay="0" data-hover="1" class="dropdown w-dropdown">
          <div class="nav-link dropdown-toggle w-dropdown-toggle">
            <div>la communauté</div>
            <div class="dropdown-icon w-icon-dropdown-toggle"></div>
          </div>
          <nav class="dropdown-list w-dropdown-list">
            <%= link_to "Les proposants", pros_path, class: 'dropdown-link w-dropdown-link' %>
            <%= link_to "Les participants", amateurs_path, class: 'dropdown-link w-dropdown-link' %>
          </nav>
        </div>
        <%= link_to 'tarif', pricing_path, class: 'nav-link w-nav-link' %>
        <% if member_signed_in? %>
          <%= link_to "Mon profil", member_path(current_member), class: 'nav-link w-nav-link' %>
          <%= link_to "Déconnexion", destroy_member_session_path, method: :DELETE, class: 'nav-link w-nav-link' %>
        <% else %>
          <%= link_to t(".sign_up", default: "Créer un compte"), new_member_registration_path, title: "Créer un compte", class: 'nav-link w-nav-link' %>
          <%= link_to t(".sign_in", default: "Connectez-vous"), new_member_session_path, title: "Connectez-vous", class: 'nav-link w-nav-link' %>
        <% end %>
      </nav>
      <div class="menu-button w-nav-button">
        <div class="w-icon-nav-menu"></div>
      </div>
    </div>
  </div>
</div>
